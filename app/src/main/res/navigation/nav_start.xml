<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_start.xml"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.electric.bunk.fragments.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_loginTypeFragment"
            app:destination="@id/loginTypeFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_splashFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/loginTypeFragment"
        android:name="com.electric.bunk.fragments.LoginTypeFragment"
        android:label="fragment_login_type"
        tools:layout="@layout/fragment_login_type" >
        <action
            android:id="@+id/action_loginTypeFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.electric.bunk.fragments.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.electric.bunk.fragments.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.electric.bunk.fragments.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_searchBunkFragment"
            app:destination="@id/searchBunkFragment"
            app:enterAnim="@anim/animate_swipe_left_enter"
            app:exitAnim="@anim/animate_swipe_left_exit"
            app:popEnterAnim="@anim/animate_swipe_left_enter"
            app:popExitAnim="@anim/animate_swipe_left_exit" />
        <action
            android:id="@+id/action_homeFragment_to_createBunkFragment"
            app:destination="@id/createBunkFragment"
            app:enterAnim="@anim/animate_slide_left_enter"
            app:exitAnim="@anim/animate_slide_left_exit"
            app:popEnterAnim="@anim/animate_slide_left_enter"
            app:popExitAnim="@anim/animate_slide_left_exit" />
        <action
            android:id="@+id/action_homeFragment_to_helpFragment"
            app:destination="@id/helpFragment"
            app:enterAnim="@anim/animate_slide_left_enter"
            app:exitAnim="@anim/animate_slide_left_exit"
            app:popEnterAnim="@anim/animate_slide_left_enter"
            app:popExitAnim="@anim/animate_slide_left_exit" />
        <action
            android:id="@+id/action_homeFragment_to_profileFragment"
            app:destination="@id/profileFragment"
            app:enterAnim="@anim/animate_slide_left_enter"
            app:exitAnim="@anim/animate_slide_left_exit"
            app:popEnterAnim="@anim/animate_slide_left_enter"
            app:popExitAnim="@anim/animate_slide_left_exit" />
        <action
            android:id="@+id/action_homeFragment_to_changeAutherizationFragment"
            app:destination="@id/changeAutherizationFragment"
            app:enterAnim="@anim/animate_slide_left_enter"
            app:exitAnim="@anim/animate_slide_left_exit"
            app:popEnterAnim="@anim/animate_slide_left_enter"
            app:popExitAnim="@anim/animate_slide_left_exit" />
        <action
            android:id="@+id/action_homeFragment_to_bookingListFragment"
            app:destination="@id/bookingListFragment"
            app:enterAnim="@anim/animate_slide_left_enter"
            app:exitAnim="@anim/animate_slide_left_exit"
            app:popEnterAnim="@anim/animate_slide_left_enter"
            app:popExitAnim="@anim/animate_slide_left_exit" />
        <action
            android:id="@+id/action_homeFragment_to_myFeedbacksFragment"
            app:destination="@id/myFeedbacksFragment"
            app:enterAnim="@anim/animate_slide_in_left"
            app:exitAnim="@anim/animate_slide_left_enter"
            app:popEnterAnim="@anim/animate_slide_left_exit"
            app:popExitAnim="@anim/animate_swipe_left_enter" />
        <action
            android:id="@+id/action_homeFragment_to_myBunksListFragment"
            app:destination="@id/myBunksListFragment"
            app:enterAnim="@anim/animate_slide_in_left"
            app:exitAnim="@anim/animate_slide_left_enter"
            app:popEnterAnim="@anim/animate_slide_left_exit"
            app:popExitAnim="@anim/animate_swipe_left_enter" />
    </fragment>
    <fragment
        android:id="@+id/searchBunkFragment"
        android:name="com.electric.bunk.fragments.SearchBunkFragment"
        android:label="fragment_search_bunk"
        tools:layout="@layout/fragment_search_bunk" >
        <action
            android:id="@+id/action_searchBunkFragment_to_bunkDetailFragment"
            app:destination="@id/bunkDetailFragment"
            app:enterAnim="@anim/animate_slide_left_enter"
            app:exitAnim="@anim/animate_slide_left_exit"
            app:popEnterAnim="@anim/animate_slide_left_enter"
            app:popExitAnim="@anim/animate_slide_left_exit" />
    </fragment>
    <fragment
        android:id="@+id/createBunkFragment"
        android:name="com.electric.bunk.fragments.CreateBunkFragment"
        android:label="fragment_create_bunk"
        tools:layout="@layout/fragment_create_bunk" />
    <fragment
        android:id="@+id/bunkDetailFragment"
        android:name="com.electric.bunk.fragments.BunkDetailFragment"
        android:label="fragment_bunk_detail"
        tools:layout="@layout/fragment_bunk_detail" />
    <fragment
        android:id="@+id/helpFragment"
        android:name="com.electric.bunk.fragments.HelpFragment"
        android:label="fragment_help"
        tools:layout="@layout/fragment_help" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.electric.bunk.fragments.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/changeAutherizationFragment"
        android:name="com.electric.bunk.fragments.ChangeAutherizationFragment"
        android:label="fragment_change_autherization"
        tools:layout="@layout/fragment_change_autherization" />
    <fragment
        android:id="@+id/bookingListFragment"
        android:name="com.electric.bunk.fragments.BookingListFragment"
        android:label="fragment_booking_list"
        tools:layout="@layout/fragment_booking_list" >
        <action
            android:id="@+id/action_bookingListFragment_to_bookingDetailsFragment"
            app:destination="@id/bookingDetailsFragment"
            app:enterAnim="@anim/animate_slide_left_enter"
            app:exitAnim="@anim/animate_slide_in_left"
            app:popEnterAnim="@anim/animate_slide_left_enter"
            app:popExitAnim="@anim/animate_slide_left_exit" />
    </fragment>
    <fragment
        android:id="@+id/bookingDetailsFragment"
        android:name="com.electric.bunk.fragments.BookingDetailsFragment"
        android:label="fragment_booking_details"
        tools:layout="@layout/fragment_booking_details" />
    <fragment
        android:id="@+id/myFeedbacksFragment"
        android:name="com.electric.bunk.fragments.MyFeedbacksFragment"
        android:label="fragment_my_feedbacks"
        tools:layout="@layout/fragment_my_feedbacks" />
    <fragment
        android:id="@+id/myBunksListFragment"
        android:name="com.electric.bunk.fragments.MyBunksListFragment"
        android:label="fragment_my_bunks_list"
        tools:layout="@layout/fragment_my_bunks_list" >
        <action
            android:id="@+id/action_myBunksListFragment_to_editNDeleteBunkFragment"
            app:destination="@id/editNDeleteBunkFragment"
            app:enterAnim="@anim/animate_slide_in_left"
            app:exitAnim="@anim/animate_slide_left_enter"
            app:popEnterAnim="@anim/animate_slide_left_exit"
            app:popExitAnim="@anim/animate_swipe_left_enter" />
    </fragment>
    <fragment
        android:id="@+id/editNDeleteBunkFragment"
        android:name="com.electric.bunk.fragments.EditNDeleteBunkFragment"
        android:label="EditNDeleteBunkFragment" />
</navigation>